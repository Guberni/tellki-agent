#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node --expose-gc" "$basedir/node_modules/tellki-agent/bin/tellki-agent" "$@"
  ret=$?
else 
  node --expose-gc "$basedir/node_modules/tellki-agent/bin/tellki-agent" "$@"
  ret=$?
fi
exit $ret